{
  "name": "wip",
  "alias": "wip",
  "scale": {
    "all": {
      "min": 1,
      "max": 2
    }
  },
  "env": {
    "APP_ID": "@app-id",
    "APP_NAME": "WIP",
    "DISABLE_STATS": "true",
    "LOG_LEVEL": "fatal",
    "LOGDNA_KEY": "@logdna-key",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@private-key",
    "REDIS_URL": "@redis-url",
    "SENTRY_DSN": "@sentry-dsn",
    "WEBHOOK_SECRET": "@webhook-secret"
  }
}
