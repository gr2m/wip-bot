{
  "name": "wip-beta",
  "alias": "wip-beta",
  "scale": {
    "all": {
      "min": 1,
      "max": 2
    }
  },
  "env": {
    "APP_ID": "@wip-app-id",
    "APP_NAME": "WIP (beta)",
    "DISABLE_STATS": "true",
    "LOG_LEVEL": "@wip-log-level",
    "LOGDNA_API_KEY": "@wip-logdna-api-key",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@wip-private-key",
    "REDIS_URL": "@wip-redis-url",
    "SENTRY_DSN": "@wip-sentry-dsn",
    "WEBHOOK_SECRET": "@wip-webhook-secret"
  }
}
