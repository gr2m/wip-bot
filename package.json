{
  "name": "semantic-pull-requests",
  "version": "0.0.0-development",
  "description": "Status check that ensures your pull requests have semantic titles",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "start": "probot run ./index.js",
    "test": "jest && npm run lint",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "pretest": "npm run lint",
    "lint": "standard",
    "semantic-release": "semantic-release"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": "https://github.com/probot/semantic-pull-requests",
  "keywords": [
    "probot",
    "probot-plugin"
  ],
  "author": "Zeke Sikelianos (https://github.com/zeke)",
  "license": "Apache-2.0",
  "dependencies": {
    "probot": "^7.0.1",
    "simple-commit-message": "^4.0.3"
  },
  "devDependencies": {
    "@octokit/rest": "^15.9.4",
    "jest": "^23.4.0",
    "localtunnel": "^1.8.3",
    "nock": "^9.4.2",
    "semantic-release": "^12.2.2",
    "smee-client": "^1.0.1",
    "standard": "^11.0.0"
  },
  "engines": {
    "node": "8"
  }
}
